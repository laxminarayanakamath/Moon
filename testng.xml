<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="classes" name="CBoosterTestSuite">

	<listeners>
		<listener class-name="com.bixbytes.qa.cbooster.ExtentReportListner.ExtentReportNG" />
		<listener class-name="com.bixbytes.qa.cbooster.utilities.ITestListner" />
		
	</listeners>
<groups>
<run>
<include name="SmokeTest" />
<include name="FunctionalPositive" />
<include name="FunctionalNegative" />
</run>
</groups>
  <test  name="SignInPagePositive">
    <classes>
         <class name="com.bixbytes.qa.cbooster.testcases.SignIn_Positive_Testcases"/>
    </classes>
  </test>
  
  <test name="SignInNegative">
  <classes>
      <class name="com.bixbytes.qa.cbooster.testcases.SignIn_Negative_Cases"/>
  </classes>
  </test>
  
  <test name="DashBoardPositive">
  <classes>
       <class name="com.bixbytes.qa.cbooster.testcases.DashBoard_Positive_Testcases"/>
    </classes>
  </test>
</suite>
